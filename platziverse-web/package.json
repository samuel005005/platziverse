{
  "name": "platziverse-web",
  "version": "1.0.0",
  "description": "A simple IoT Platform - Web Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "start-dev": "cross-env DEBUG=platziverse:* nodemon --exec babel-node server",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "prestart": "npm run build",
    "prestart-dev": "npm run build-dev",
    "prestart:-ev:watch": "npm run build",
    "build-dev": "webpack --mode development",
    "build": "cross-env NODE_ENV=production browserify -g envify -t babelify -t vueify -e client/app.js | terser -c -m -o public/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuel005005/platziverse.git"
  },
  "author": "Samuel Paez <samuel005005@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samuel005005/platziverse/issues"
  },
  "homepage": "https://github.com/samuel005005/platziverse#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "standard": "^17.0.0",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.13",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "chalk": "4.1.2",
    "chart.js": "^3.9.1",
    "vue-chartjs": "^4.1.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "vue": "^3.2.41",
    "platziverse-agent": "file:../platziverse-agent",
    "platziverse-utils": "file:../platziverse-utils",
    "random-material-color": "^1.0.5",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3"
  }
}